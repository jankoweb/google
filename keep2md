javascript:(function(){let selectedNotes=document.querySelectorAll('div.IZ65Hb-n0tgWb.IZ65Hb-WsjYwc-nUpftc');let markdownOutput='';selectedNotes.forEach(note=>{let cancelButton=note.querySelector('div[aria-label="Zrušit výběr poznámky"]');if(cancelButton){let textboxes=note.querySelectorAll('div[role="textbox"]');if(textboxes.length>0){let title=textboxes[0].innerText.trim();let content=textboxes.length>1?textboxes[1].innerText.replace(/\n+$/g,"").trim():"";if(content){content=content.replace(/\n+/g," // ").trim();}if(title && content){markdownOutput+="- [ ] "+title+": "+content+"\n";}else if(title){markdownOutput+="- [ ] "+title+"\n";}else if(content){markdownOutput+="- [ ] "+content+"\n";}}}});if(markdownOutput){console.log(markdownOutput);}else{console.log("Nebyly nalezeny žádné vybrané poznámky.");}})();
